{
  "name": "cw-dao-dapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "codegen": "npx ts-node --project tsconfig.tools.json tools/codegen.ts",
    "dev": "next dev",
    "build": "next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint && prettier --check .",
    "prettier:write": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "ts": "tsc --noEmit --incremental",
    "ts:watch": "yarn ts --watch",
    "schema:generate": "npx json-schema-to-typescript -i ../cw-dao/contracts/cw-dao/schema -o \"@types/${PWD##*/}\" && npx barrelsby --delete -s -q -d \"@types/${PWD##*/}\""
  },
