{
  "name": "dao-dao",
  "version": "0.3.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --continue",
    "dev": "yarn dapp dev",
    "format": "prettier --write \"**/*.{css,json,md}\" && turbo run format --continue",
    "lint": "prettier --check \"**/*.{css,json,md}\" && turbo run lint --continue",
    "test": "turbo run test",
    "dapp": "yarn workspace @dao-dao/dapp",
    "raw-sda": "yarn workspace @dao-dao/raw-sda",
    "sda-base": "yarn workspace @dao-dao/sda-base",
    "icons": "yarn workspace @dao-dao/icons",
    "state": "yarn workspace @dao-dao/state",
    "ui": "yarn workspace @dao-dao/ui",
    "utils": "yarn workspace @dao-dao/utils",
    "actions": "yarn workspace @dao-dao/actions",
    "common": "yarn workspace @dao-dao/common",
    "i18n": "yarn workspace @dao-dao/i18n",
    "storybook": "yarn workspace @dao-dao/storybook"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "prettier": "@dao-dao/config/prettier",
  "packageManager": "yarn@1.22.11",
  "private": true
}
