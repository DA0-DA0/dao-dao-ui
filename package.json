{
  "name": "dao-dao",
  "version": "0.3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint && prettier --check \"**/*.{css,json,md}\"",
    "format": "turbo run format && prettier --write \"**/*.{css,json,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "prettier": "@project/prettier-config",
  "packageManager": "yarn@1.22.11"
}
