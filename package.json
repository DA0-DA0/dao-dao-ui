    "@types/jsoneditor": "^9.5.1",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "17.0.19",
    "autoprefixer": "^10.3.3",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "jest": "^27.3.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.7",
    "typescript": "4.3.5"
  }
