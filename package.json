    "daisyui": "^1.14.0",
    "dotenv": "^10.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "jsoneditor": "^9.5.6",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "next": "11.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.1.1",
    "react-json-editor-ajrm": "^2.5.13",
